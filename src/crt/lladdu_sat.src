	assume	adl=1

	section	.text

	public	__lladdu_sat

__lladdu_sat:
	; unsigned saturated addition
	; BC:UDE:UHL = BC:UDE:UHL + (SP)
	push	iy
	ld	iy, 0
	add	iy, sp
	push	bc
	ld	bc, (iy + 6)
	add	hl, bc
	ex	de, hl
	ld	bc, (iy + 9)
	adc	hl, bc
	ex	de, hl
	pop	bc
	jr	nc, .nc48
	inc	bc
.nc48:
	ld	iy, (iy + 12)
	add.s	iy, bc
	lea	bc, iy
	pop	iy
	ret	nc
	sbc	hl, hl
	ex	de, hl
	sbc	hl, hl
	ld	b, l
	ld	c, l
	ret
