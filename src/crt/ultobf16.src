	assume	adl=1

	section	.text

	public	__ultobf16

	; (__bf16)unsigned long
; input E:UHL output HL
__ultobf16:
	push	af, bc	; preserve
	
	; convert calling conventions
	push	hl
	pop	bc
	ld	a, e
	call	__ultof
	ld	e, a
	push	bc
	pop	hl
	
	pop	af, bc	; restore
	
	call	__ftobf16

extern	__ultof
extern	__ftobf16
