	assume	adl=1

	section	.text

	public	__i48subs_sat

__i48subs_sat:
	; signed saturated subtraction
	; UDE:UHL = UDE:UHL - UIY:UBC
	or	a, a
	sbc	hl, bc
	ex	de, hl
	push	bc
	lea	bc, iy
	sbc	hl, bc
	pop	bc
	ex	de, hl
	ret	po
	ld	hl, $000000
	ld	de, $800000
	ret	p	; negative underflow
	; positive overflow
	dec	hl
	dec	de
	ret
