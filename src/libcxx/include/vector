// -*- C++ -*-
#ifndef _EZCXX_VECTOR
#define _EZCXX_VECTOR

#include <EASTL/vector.h>
#include <memory>

#pragma clang system_header

namespace std {

template <typename _Tp, typename _Allocator = std::allocator<_Tp>>
using vector = eastl::vector<_Tp, _Allocator>;

} // namespace std

#endif // _EZCXX_VECTOR
