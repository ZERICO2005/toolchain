// -*- C++ -*-
#ifndef _EZCXX_DEQUE
#define _EZCXX_DEQUE

#include <EASTL/deque.h>
#include <initializer_list>
#include <memory>

#pragma clang system_header

namespace std {

template <typename _Tp, typename _Allocator = std::allocator<_Tp>>
using deque = eastl::deque<_Tp, _Allocator>;

} // namespace std

#endif // _EZCXX_DEQUE
