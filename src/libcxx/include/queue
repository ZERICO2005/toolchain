// -*- C++ -*-
#ifndef _EZCXX_QUEUE
#define _EZCXX_QUEUE

#include <EASTL/queue.h>
#include <EASTL/priority_queue.h>
#include <deque>
#include <vector>
#include <initializer_list>
#include <memory>
#include <functional>

#pragma clang system_header

namespace std {

template <typename _Tp, typename _Container = std::deque<_Tp>>
using queue = eastl::queue<_Tp, _Container>;

template <
	typename _Tp,
	typename _Container = std::vector<_Tp>,
	typename _Compare = std::less<typename _Container::value_type>
>
using priority_queue = eastl::priority_queue<_Tp, _Container, _Compare>;

} // namespace std

#endif // _EZCXX_QUEUE
