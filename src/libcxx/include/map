// -*- C++ -*-
#ifndef _EZCXX_MAP
#define _EZCXX_MAP

#include <EASTL/map.h>
#include <functional>
#include <pair>
#include <utility>

#pragma clang system_header

namespace std {

template <
    class _Key,
    class _Tp,
    class _Compare = std::less<_Key>,
    class _Allocator = void
>
using map = eastl::map<_Key, _Tp, _Compare>;

template <
    class _Key,
    class _Tp,
    class _Compare = std::less<_Key>,
    class _Allocator = void
>
using multimap = eastl::multimap<_Key, _Tp, _Compare>;

} // namespace std

#endif // _EZCXX_MAP
