// -*- C++ -*-
#ifndef _EZCXX_MAP
#define _EZCXX_MAP

#include <EASTL/map.h>

#pragma clang system_header

namespace std {

template <
    class _Key,
    class _Tp,
    class _Compare,
    class _Allocator
>
using map = eastl::map<_Key, _Tp, _Compare, _Allocator>;

template <
    class _Key,
    class _Tp,
    class _Compare,
    class _Allocator
>
using multimap = eastl::multimap<_Key, _Tp, _Compare, _Allocator>;

} // namespace std

#endif // _EZCXX_MAP
