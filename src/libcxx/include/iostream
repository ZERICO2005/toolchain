// -*- C++ -*-
#ifndef _EZCXX_IOSTREAM
#define _EZCXX_IOSTREAM

#include <ios>
#include <streambuf>
#include <istream>
#include <ostream>

#pragma clang system_header

namespace std {
	extern istream cin;
	extern ostream cout;
	extern ostream cerr;
	extern ostream clog;

	extern wistream wcin;
	extern wostream wcout;
	extern wostream wcerr;
	extern wostream wclog;
}

#endif // _EZCXX_IOSTREAM
