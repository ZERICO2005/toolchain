// -*- C++ -*-
#ifndef _EZCXX_TUPLE
#define _EZCXX_TUPLE

#include <__config>
#include <cstddef>
#include <EASTL/tuple.h>
#include <EASTL/utility.h>
#include <pair>

#pragma clang system_header

namespace std {

template<typename... _Types>
using tuple = eastl::tuple<_Types...>;

template<typename... _Types>
using tuple_size = eastl::tuple_size<_Types...>;

template<std::size_t _Ip, typename... _Types>
using tuple_element = eastl::tuple_element<_Ip, _Types...>;

template<class _Tp>
constexpr std::size_t tuple_size_v = tuple_size<_Tp>::value;

} // namespace std

#endif // _EZCXX_TUPLE
